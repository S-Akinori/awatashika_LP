---
import Welcome from '../components/Welcome.astro';
import Layout from '../layouts/Layout.astro';
import Header from '@/components/Header.tsx';
import "../styles/global.css";
import FV from '@/components/FV.astro';
import MessageSection from '@/components/MessageSection.astro';
import SectionWrapper from '@/components/SectionWrapper.astro';
import FeatureSection from '@/components/FeatureSection.astro';
import DetailSection from '@/components/DetailSection.astro';
import ReviewSection from '@/components/ReviewSection.astro';
import OwnerSection from '@/components/OwnerSection.astro';
